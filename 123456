CREATE DATABASE escola;

use escola;
CREATE TABLE ALUNO (
  idALUNO INTEGER UNSIGNED  NOT NULL   AUTO_INCREMENT,
  IDADE VARCHAR(255)  NOT NULL  ,
  SOBRENOMNE VARCHAR(255)  NOT NULL  ,
  EMAIL VARCHAR(255)  NOT NULL    ,
PRIMARY KEY(idALUNO));

CREATE TABLE INSCRIÇÃO (
  idINSCRIÇÃO INTEGER UNSIGNED  NOT NULL   AUTO_INCREMENT,
  ALUNO_idALUNO INTEGER UNSIGNED  NOT NULL  ,
  DATA_INSCRIÇÃO VARCHAR(50)  NOT NULL  ,
  STATUS_2 VARCHAR(50)  NOT NULL    ,
PRIMARY KEY(idINSCRIÇÃO)  ,
INDEX INSCRIÇÃO_FKIndex1(ALUNO_idALUNO));

CREATE TABLE CURSO (
  idCURSO INTEGER UNSIGNED  NOT NULL   AUTO_INCREMENT,
  INSCRIÇÃO_idINSCRIÇÃO INTEGER UNSIGNED  NOT NULL  ,
  PROFESSOR_idPROFESSOR INTEGER UNSIGNED  NOT NULL  ,
  TITULO VARCHAR(255)  NOT NULL  ,
  DESCRIÇÃO VARCHAR(255)  NOT NULL  ,
  CATEGORIA VARCHAR(255)  NOT NULL  ,
  NIVEL_DE_DIFICULDADE VARCHAR(50)  NOT NULL  ,
  POPULARIDADE VARCHAR(50)  NOT NULL    ,
PRIMARY KEY(idCURSO)  ,
INDEX CURSO_FKIndex1(PROFESSOR_idPROFESSOR)  ,
INDEX CURSO_FKIndex2(INSCRIÇÃO_idINSCRIÇÃO));

CREATE TABLE PROFESSOR (
  idPROFESSOR INTEGER UNSIGNED  NOT NULL   AUTO_INCREMENT,
  NOME VARCHAR(255)  NOT NULL  ,
  SOBRENOME VARCHAR(255)  NOT NULL  ,
  EMAIL VARCHAR(255)  NOT NULL    ,
PRIMARY KEY(idPROFESSOR));

select*from ALUNO;



